<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<ScrollPane fx:id="loginAlliesComponent" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.login.LoginControllerAllies">
    <content>
        <AnchorPane prefHeight="800.0" prefWidth="1200.0">
            <children>
                <Button fx:id="loginButton" layoutX="327.0" layoutY="169.0" mnemonicParsing="false" onAction="#loginButtonListener" prefHeight="50.0" prefWidth="200.0" text="Login">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
                <Label fx:id="loginErrorLabel" layoutX="176.0" layoutY="219.0" prefHeight="52.0" prefWidth="1006.0" textFill="#f21212">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font></Label>
                <Label alignment="TOP_CENTER" layoutX="1.0" layoutY="36.0" prefHeight="35.0" prefWidth="869.0" text="Allies - Login Page">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Label>
            <Label layoutX="213.0" layoutY="110.0" prefHeight="27.0" prefWidth="101.0" text="Username">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="loginTextField" layoutX="327.0" layoutY="111.0" prefHeight="25.0" prefWidth="250.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </TextField>
            </children>
        </AnchorPane>
    </content>
</ScrollPane>
